STARTUP(out/startup.o)
ENTRY(_Reset)

MEMORY {
    RAM  (RWX) : ORIGIN = 0x40004000, LENGTH = 16K
    RAMm (RWX) : ORIGIN = 0x40008000, LENGTH = 32K
}

SECTIONS {

    .text : {
        *(.text) 
    } > RAM

    .data : {
        . = ALIGN(4);
        *(.rodata)
        *(.data)
        __e_loader = ALIGN(8);
    } > RAM

    .bss : {
        *(.bss)
    } > RAM

    .mainLoader : {
        __s_mainLoader = . ;
    } > RAMm


    __s_stack = 0x40002000 - 24;
}


